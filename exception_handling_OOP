#include<iostream>
using namespace std;
int main()
{
    float x,y;
    cout<<"Enter the value of x and y :";
    cin>>x>>y;
    try{
        if(y!=0){
            float a = x/y;
            cout<<"division is :"<<a;
        }
        else{
            throw x;
        }
    }
    catch(int){
        cout<<"Exception is caught:"<<"x = "<<x<<endl<<"y = "<<y;
    }

return 0;    
} 
