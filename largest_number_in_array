#include<stdio.h>
int main(){
double arr[100];
int n;
printf("Enter the value of n : ");
scanf("%d",&n);
	for(int i=0;i<n;i++){
	printf("Enter the element of your array : ");
	scanf("%lf",arr);
		}
	for(int i=0;i<n;i++){
		if(arr[0]<arr[i]){
		arr[0] = arr[i];
		}
	
	}
printf("Largest element of array is %lf ",arr[0]);
return 0;
}
