#include<iostream>
using namespace std;
class sum{
    public:
    int a = 10;
    int b = 30;
    sum(int a,int b){
        cout<<"Sum of a and b is :"<<a+b<<endl;
    }
    sum(sum &s2){
        cout<<"Sum is:"<<a+b<<endl;
    }

};
int main(){
    sum s1(10,30);
    sum s2(s1);


    return 0;
}
